{
  "name": "@nejcm/react-skeleton",
  "version": "0.0.0-semantically-released",
  "author": "Nejc Mursic <nmursi2@gmail.com> (https://github.com/nejcm)",
  "description": "React skeleton loader",
  "source": "src/index.js",
  "main": "dist/index.js",
  "umd:main": "dist/index.umd.js",
  "module": "dist/inded.esm.js",
  "typings": "typings/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nejcm/react-skeleton"
  },
  "bugs": {
    "url": "https://github.com/nejc/react-skeleton/issues"
  },
  "homepage": "https://github.com/nejcm/react-skeleton",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "keywords": [
    "js",
    "es6",
    "react",
    "library"
  ],
  "files": [
    "dist"
  ],
  "scripts": {
    "storybook": "start-storybook -p 9001 -c .storybook",
    "storybook:build": "build-storybook -c .storybook -o .storybook-static",
    "storybook:deploy": "npm run storybook:build && gh-pages -d .storybook-static",
    "format": "prettier ./src/**/*.js --write",
    "clean": "rimraf dist coverage .storybook-static",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "test": "npm run lint && jest",
    "test:cov": "jest --coverage --colors && jest-coverage-badges",
    "test:build": "jest --coverage --colors --silent && jest-coverage-badges",
    "test:watch": "jest --watch",
    "build": "npm run clean && rollup -c",
    "validate": "npm run format && npm run lint && npm run test:build",
    "setup": "npm install && npm run validate",
    "semantic-release": "semantic-release"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run validate"
    }
  },
  "peerDependencies": {
    "prop-types": ">= 14",
    "react": ">= 16",
    "react-dom": ">= 16"
  },
  "dependencies": {
    "@babel/runtime": "^7.4.5",
    "prop-types": "^15.7.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@semantic-release/changelog": "^5.0.0",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.0.4",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "@semantic-release/npm": "^7.0.4",
    "@storybook/addon-actions": "^5.3.17",
    "storybook-addon-jsx": "^7.1.15",
    "@storybook/addon-knobs": "^5.3.17",
    "@storybook/addon-links": "^5.3.17",
    "@storybook/addon-notes": "^5.3.17",
    "@storybook/addon-options": "^5.3.17",
    "@storybook/addons": "^5.3.17",
    "@storybook/react": "^5.3.17",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^10.0.1",
    "@testing-library/react-hooks": "^3.2.1",
    "@testing-library/user-event": "^10.0.0",
    "autoprefixer": "^9.7.4",
    "babel-plugin-minify-dead-code-elimination": "^0.5.1",
    "babel-plugin-no-side-effect-class-properties": "0.0.7",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "codecov": "^3.6.5",
    "cross-env": "^7.0.2",
    "@emotion/styled": "^10.0.27",
    "eslint-config-kentcdodds": "^14.8.0",
    "eslint-plugin-react": "7.19.0",
    "gh-pages": "^2.2.0",
    "husky": "4.2.3",
    "jest": "25.1.0",
    "jest-coverage-badges": "^1.1.2",
    "jest-environment-jsdom-fourteen": "1.0.1",
    "jest-resolve": "25.1.0",
    "jest-watch-typeahead": "0.4.2",
    "lint-staged": "10.0.8",
    "node-sass": "^4.13.1",
    "postcss": "^7.0.27",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-test-renderer": "^16.13.0",
    "rimraf": "^3.0.2",
    "rollup": "2.1.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-filesize": "^6.2.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-sass": "^1.2.2",
    "rollup-plugin-uglify": "^6.0.4",
    "sass-loader": "^8.0.2",
    "semantic-release": "17.0.4",
    "start-server-and-test": "^1.10.11",
    "style-loader": "^1.1.3"
  }
}
